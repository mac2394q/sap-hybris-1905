<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	<itemtypes>
<itemtype code="Address" autocreate="false" generate="false">
         	<attributes>
         		<attribute qualifier="street" type="java.lang.String">
                	<description>street from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="adCity" type="java.lang.String">
                	<description>city from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="state" type="java.lang.String">
                	<description>state from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="contactName" type="java.lang.String">
                	<description>contact name from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="companyName" type="java.lang.String">
                	<description>company name from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="countryName" type="java.lang.String">
                	<description>country name from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="streetAppendix" type="java.lang.String">
                	<description>streetAppendix from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="ownerType" type="java.lang.String">
                	<description>ownerType from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="isDefault" type="java.lang.Boolean">
                	<description>is default from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            	<attribute qualifier="isDelete" type="java.lang.Boolean">
                	<description>is delete from the Commerce System</description>
                    <persistence type="property"/>
            	</attribute>
            </attributes>
         </itemtype>
         
