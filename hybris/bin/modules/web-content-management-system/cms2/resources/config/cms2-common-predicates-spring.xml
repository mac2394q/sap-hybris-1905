<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <alias name="cmsComponentTypePredicate" alias="cmsComponentPredicate" />
    <bean id="cmsComponentTypePredicate" class="de.hybris.platform.cms2.common.predicates.CMSComponentTypePredicate">
        <property name="typeService" ref="typeService" />
    </bean>

    <alias name="defaultContentSlotPredicate" alias="cmsContentSlotPredicate" />
    <bean id="defaultContentSlotPredicate" class="de.hybris.platform.cms2.common.predicates.ContentSlotPredicate">
        <property name="typeService" ref="typeService" />
    </bean>

    <alias name="defaultRestrictionPredicate" alias="cmsRestrictionPredicate" />
    <bean id="defaultRestrictionPredicate" class="de.hybris.platform.cms2.common.predicates.RestrictionPredicate">
        <property name="typeService" ref="typeService" />
    </bean>

    <alias name="defaultContentSlotForPagePredicate" alias="cmsContentSlotForPagePredicate" />
    <bean id="defaultContentSlotForPagePredicate" class="de.hybris.platform.cms2.common.predicates.ContentSlotForPagePredicate">
        <property name="typeService" ref="typeService" />
    </bean>

    <alias name="defaultCMSWorkflowCommentPredicate" alias="cmsWorkflowCommentPredicate" />
    <bean id="defaultCMSWorkflowCommentPredicate" class="de.hybris.platform.cms2.common.predicates.CMSWorkflowCommentPredicate">
